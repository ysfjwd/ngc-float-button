{
  "name": "ngc-float-button",
  "description": "Google Material Float Button - Implementation for Angular v4+",
  "version": "6.0.0",
  "scripts": {
    "transpile": "ngc",
    "package": "rollup -c",
    "minify":
      "uglifyjs dist/bundles/ngc-float-button.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/ngc-float-button.umd.min.js",
    "build": "npm run transpile && npm run package && npm run minify"
  },
  "keywords": ["angular2", "angular", "npm", "package", "module", "float button", "float", "button"],
  "author": "Gustavo Costa - @GustavoCostaW",
  "license": "MIT",
  "peerDependencies": {
    "@angular/core": ">=6.0.0"
  },
  "devDependencies": {
    "@angular/cdk": "^6.0.0",
    "@angular/common": "6.0.0",
    "@angular/compiler": "6.0.0",
    "@angular/compiler-cli": "6.0.0",
    "@angular/platform-browser": "6.0.0",
    "@angular/platform-browser-dynamic": "6.0.0",
    "@angular/core": "6.0.0",
    "@angular/forms": "6.0.0",
    "@angular/material": "^6.0.0",
    "@angular/animations": "6.0.0",
    "@angular/http": "6.0.0",
    "@types/node": "^8.5.9",
    "core-js": "^2.4.1",
    "rollup": "^0.50.0",
    "rxjs": "^6.0.0",
    "typescript": ">=2.7.2 <2.8",
    "uglify-js": "^3.3.9",
    "zone.js": "^0.8.4"
  }
}
